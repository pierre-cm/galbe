{
  "name": "galbe",
  "version": "0.9.0",
  "description": "Fast, lightweight and highly customizable JavaScript web framework based on Bun",
  "author": "Pierre Caillaud M (https://github.com/pierre-cm)",
  "type": "module",
  "bin": "./bin/cli.ts",
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./schema": "./src/schema.ts",
    "./extras": "./src/extras.ts",
    "./utils": "./src/util.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pierre-cm/galbe"
  },
  "bugs": "https://github.com/pierre-cm/galbe/issues",
  "homepage": "https://galbe.dev",
  "keywords": [
    "bun",
    "http",
    "web",
    "framework",
    "server",
    "api"
  ],
  "license": "MIT",
  "scripts": {
    "test": "bun test",
    "postinstall": "bun run ./scripts/postinstall.ts",
    "release": "release-it"
  },
  "devDependencies": {
    "@types/bun": "^1.0.4",
    "openapi-types": "^12.1.3",
    "release-it": "^17.1.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@swc/core": "^1.3.107",
    "@swc/wasm": "^1.4.0",
    "acorn": "^8.11.2",
    "acorn-walk": "^8.3.0",
    "chokidar": "^3.6.0",
    "commander": "^11.1.0",
    "js-yaml": "^4.1.0"
  },
  "release-it": {
    "git": {
      "pushRepo": "git@github.com:pierre-cm/galbe.git"
    },
    "github": {
      "requireBranch": "main",
      "release": "true"
    }
  }
}
